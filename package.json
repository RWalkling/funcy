    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "typescript": "^3.5.3",
    "eslint": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
